<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="movies">
  	
  	<!-- SQL구문에 넣어주는 자료형 -->
  	<delete id="delete" parameterType="int">
  		DELETE movies
  		where no = #{no}
  		<!-- #{} 는 변수처럼 작용 -->
  	</delete>
  
  
  </mapper>